{
  "name": "hardphp/thinkphp-snowflake",
  "description": "thinkphp6.0 snowflake,使用snowflake算法生成全局唯一且趋势递增的ID",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1.0",
    "topthink/framework": "^6.0.0",
    "topthink/think-factory": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "hardphp\\snowflake\\": "src/",
      "think\\facade\\": "src/facade/"
    }
  },
  "extra": {
    "think": {
      "services": [
        "hardphp\\snowflake\\Service"
      ],
      "config": {
        "snowflake": "src/config.php"
      }
    }
  }
}
